<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block class="Xigen\Announce\Block\Announcement" name="annouce.contact.content.top" before="-">
                <action method="setPosition">
                    <argument name="position" xsi:type="string">contact-content-top</argument>
                </action>
            </block>
            <referenceContainer name="form.additional.info" as="form_additional_info">
                <block class="Xigen\Announce\Block\Announcement" name="annouce.contact.content.form">
                    <action method="setPosition">
                        <argument name="position" xsi:type="string">contact-content-form</argument>
                    </action>
                </block>
            </referenceContainer>
        </referenceContainer>
    </body>
</page>